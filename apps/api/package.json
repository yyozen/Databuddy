{
  "name": "api",
  "version": "1.0.50",
  "packageManager": "bun@1.2.3",
  "scripts": {
    "dev": "bun --watch run src/index.ts",
    "clean": "rm -rf dist",
    "env": "dotenv -e .dev.vars",
    "build": "bun clean",
    "start": "bun run src/index.ts",
    "pm2": "pm2 start src/index.ts --interpreter bun --name databuddy-api",
    "pm2:stop": "pm2 stop databuddy-api",
    "pm2:restart": "pm2 restart databuddy-api",
    "deploy": "wrangler deploy --minify",
    "dry-run": "wrangler deploy --minify --dry-run",
    "migrate:events": "bun run src/commands/run-events-migration.ts",
    "init:tables": "bun run src/commands/init-tables.ts",
    "test": "bun run src/scripts/test.ts"
  },
  "dependencies": {
    "@clickhouse/client": "^1.10.1",
    "@databuddy/auth": "../../packages/auth",
    "@databuddy/redis": "../../packages/redis",
    "@highlight-run/hono": "^1.0.3",
    "@hono/node-server": "^1.8.2",
    "@hono/zod-validator": "^0.4.3",
    "@logtail/edge": "^0.5.2",
    "@types/ua-parser-js": "^0.7.39",
    "better-auth": "^1.2.2",
    "hono": "^4.7.2",
    "pg": "^8.15.6",
    "react": "19.0.0",
    "ua-parser-js": "^2.0.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "bun-types": "latest",
    "wrangler": "4"
  },
  "module": "src/index.js"
}