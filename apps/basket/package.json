{
  "name": "basket",
  "version": "1.0.50",
  "packageManager": "bun@1.2.12",
  "scripts": {
    "dev": "bun --watch run src/index.ts",
    "clean": "rm -rf dist",
    "env": "dotenv -e .dev.vars",
    "build": "bun clean",
    "start": "bun run src/index.ts",
    "pm2": "pm2 start src/index.ts --interpreter bun --name databuddy-api",
    "pm2:stop": "pm2 stop databuddy-api",
    "pm2:restart": "pm2 restart databuddy-api",
    "deploy": "wrangler deploy --minify",
    "dry-run": "wrangler deploy --minify --dry-run",
    "migrate:events": "bun run src/commands/run-events-migration.ts",
    "init:tables": "bun run src/commands/init-tables.ts",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@clickhouse/client": "^1.11.1",
    "@clickhouse/client-web": "^1.11.1",
    "@databuddy/auth": "../../packages/auth",
    "@databuddy/redis": "../../packages/redis",
    "@elysiajs/cors": "^1.3.3",
    "@logtail/edge": "^0.5.4",
    "@types/ua-parser-js": "^0.7.39",
    "dayjs": "^1.11.13",
    "elysia": "^1.3.4",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "stripe": "^18.2.1",
    "tldts": "^7.0.7",
    "ua-parser-js": "^2.0.3",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "bun-types": "latest",
    "wrangler": "^4.13.2",
    "vitest": "^3.2.4",
    "@vitest/ui": "^1.0.0",
    "c8": "^8.0.0"
  },
  "module": "src/index.js"
}