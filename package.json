  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@ai-sdk/react": "^1.2.12",
    "@logtail/edge": "^0.5.5",
    "@openrouter/ai-sdk-provider": "^0.7.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.9",
    "@types/ua-parser-js": "^0.7.39",
    "ai": "^4.3.16",
    "compressorjs": "^1.2.1",
    "dayjs": "^1.11.13",
    "elysia": "^1.3.4",
    "idb": "^8.0.3",
    "ioredis": "^5.6.1",
    "jotai": "^2.12.4",
    "nanoid": "^5.1.5",
    "nuqs": "^2.4.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "recharts": "^2.15.3",
    "resend": "^4.4.1",
    "superjson": "^2.2.2",
